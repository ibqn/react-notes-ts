{
  "name": "react-notes",
  "version": "1.0.0",
  "description": "Making notes. Written with React",
  "repository": "ibqn/react-notes",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --names \"WATCH,SERVE\" --prefix-colors \"bgYellow.bold,bgGreen.bold\" \"npm run watch\" \"npm run serve\"",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "tsc": "tsc --project tsconfig.json",
    "serve": "lite-server",
    "tsc:watch": "npm run tsc -- --watch"
  },
  "keywords": [],
  "author": "ibqn",
  "license": "ISC",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.32",
    "@types/react": "^16.0.9",
    "@types/react-dom": "^16.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "concurrently": "^3.5.0",
    "lite-server": "^2.3.0",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  }
}
