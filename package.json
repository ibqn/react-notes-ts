{
  "name": "react-notes",
  "version": "1.0.0",
  "description": "Making notes. Written with React",
  "repository": "ibqn/react-notes",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --names \"WATCH,SERVE\" --prefix-colors \"bgYellow.bold,bgGreen.bold\" \"npm run watch\" \"npm run serve\"",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "tsc": "tsc --project tsconfig.json",
    "serve": "lite-server",
    "tsc:watch": "npm run tsc -- --watch"
  },
  "keywords": [],
  "author": "ibqn",
  "license": "ISC",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.2.0",
    "lite-server": "^2.3.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
